{
  "name": "exampleLoader",
  "version": "0.1.0",
  "description": "Example loader widget to allow visualization and selection of available example notebooks",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/ESA-VirES/exampleLoader",
  "bugs": {
    "url": "https://github.com/ESA-VirES/exampleLoader/issues"
  },
  "license": "MIT",
  "author": "Daniel Santillan",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/my_name/myextension.git"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0",
    "@jupyterlab/apputils": "^3.0",
    "@jupyterlab/docmanager": "^3.0",
    "@jupyterlab/launcher": "^3.0",
    "@jupyterlab/mainmenu": "^3.0",
    "@jupyterlab/services": "^6.1"
  },
  "devDependencies": {
    "rimraf": "~2.6.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.3"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
